#!/bin/sh
################################################################################
# mythdb.buffer.create
#
# This script creates a buffer for accessing the MythTV database.
################################################################################
. /etc/rc.d/functions

start() {
    mm_mythdb_buffer_create
    
    return 0
}

stop() {
    return 0
}

case $1 in
    start) start ;;
    stop)  stop  ;;
esac

exit 0
