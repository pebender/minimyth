diff -Naur nss-3.14-old/mozilla/security/coreconf/Linux.mk nss-3.14-new/mozilla/security/coreconf/Linux.mk
--- nss-3.14-old/mozilla/security/coreconf/Linux.mk	2012-10-22 20:50:06.000000000 -0700
+++ nss-3.14-new/mozilla/security/coreconf/Linux.mk	2012-10-22 20:51:10.000000000 -0700
@@ -14,11 +14,11 @@
 	IMPL_STRATEGY = _PTH
 endif
 
-CC			= gcc
-CCC			= g++
-RANLIB			= ranlib
+CC			= @GAR_CC@
+CCC			= @GAR_CXX@
