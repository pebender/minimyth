+++ nss-3.14-new/mozilla/security/coreconf/UNIX.mk	2012-10-22 20:51:10.000000000 -0700
@@ -6,7 +6,7 @@
 XP_DEFINE  += -DXP_UNIX
 LIB_SUFFIX  = a
