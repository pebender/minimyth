diff -Naur sdlmame0134-old/src/osd/sdl/sdl.mak sdlmame0134-new/src/osd/sdl/sdl.mak
--- sdlmame0134-old/src/osd/sdl/sdl.mak	2009-09-10 14:20:33.000000000 -0700
+++ sdlmame0134-new/src/osd/sdl/sdl.mak	2009-09-10 14:21:07.000000000 -0700
@@ -146,7 +146,7 @@
 endif
 
 ifeq ($(TARGETOS),unix)
-DEFS += -DSDLMAME_UNIX
+DEFS += -DSDLMAME_UNIX -DNO_DEBUGGER
 ifndef NO_X11
 DEFS += -DSDLMAME_X11
 else
@@ -278,14 +278,14 @@
 
 # the new debugger relies on GTK+ in addition to the base SDLMAME needs
 # Non-X11 builds can not use the debugger
-ifndef NO_X11
-OSDCOREOBJS += $(SDLOBJ)/debugwin.o $(SDLOBJ)/dview.o $(SDLOBJ)/debug-sup.o $(SDLOBJ)/debug-intf.o
-CFLAGS += `pkg-config --cflags gtk+-2.0` `pkg-config --cflags gconf-2.0` 
