+libdir='@GAR_libdir@'
+libexecdir='@GAR_libexecdir@'
+localedir='@GAR_datadir@/locale'
+localstatedir='@GAR_localstatedir@'
+mandir='@GAR_mandir@'
+sbindir='@GAR_sbindir@'
+sharedstatedir='@GAR_sharedstatedir@'
+sysconfdir='@GAR_sysconfdir@'
