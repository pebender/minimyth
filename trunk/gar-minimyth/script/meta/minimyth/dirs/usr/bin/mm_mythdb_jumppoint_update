#!/bin/sh
################################################################################
# mm_mythdb_jumppoint_update
################################################################################
[ -n "`mm_var_get MM_DEBUG`" ] && set -x

DESTINATION=$1
KEYLIST=$2
HOSTNAME=`hostname`

mm_mythdb_command_run "UPDATE jumppoints SET keylist=\"${KEYLIST}\" \
    WHERE hostname=\"${HOSTNAME}\" AND destination=\"${DESTINATION}\""
