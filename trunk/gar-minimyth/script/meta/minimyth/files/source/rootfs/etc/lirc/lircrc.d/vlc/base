begin
    prog = vlc
    button = KEY_STOP
    config = key-stop
end

begin
    prog = vlc
    remote = devinput.mcekbd
    button = KEY_LEFTCTRL
    button = KEY_LEFTSHIFT
    button = KEY_S
    config = key-stop
end

