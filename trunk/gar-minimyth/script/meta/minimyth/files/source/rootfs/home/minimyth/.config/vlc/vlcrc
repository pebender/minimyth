[yuv] # YUV video output

# string (device, fifo or filename)
#yuv-file=stream.yuv

# string (Chroma used.)
#yuv-chroma=

# boolean (YUV4MPEG2 header (default disabled))
#yuv-yuv4mpeg2=0

